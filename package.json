{
	"name": "satisfactory-notebook",
	"version": "0.1.0",
	"homepage": "http://satisfactory-notebook.com",
	"private": true,
	"dependencies": {
		"@panelbear/panelbear-js": "^1.2.0",
		"@sentry/react": "^6.3.5",
		"@sentry/tracing": "^6.3.5",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"fraction.js": "^4.0.13",
		"react": "^0.0.0-experimental-d1542de3a",
		"react-dom": "^0.0.0-experimental-d1542de3a",
		"react-scripts": "4.0.1",
		"source-map-explorer": "^2.5.2",
		"web-vitals": "^0.2.4"
	},
	"scripts": {
		"analyze": "source-map-explorer 'build/static/js/*.js'",
		"start": "craco start",
		"build": "craco build",
		"build-full": "yarn generate && craco build",
		"build-profile": "craco build --profile",
		"test": "craco test",
		"eject": "react-scripts eject",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"generate": "node scripts/v4.js",
		"generate-v3": "node scripts/v3.js",
		"watch": "chokidar \"./scripts/**.js\" -c \"yarn run generate\""
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@craco/craco": "^6.0.0",
		"chokidar-cli": "^2.1.0",
		"eslint": "^7.17.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"gh-pages": "^3.1.0",
		"jsonfile": "^6.1.0",
		"linaria": "^3.0.0-beta.1"
	}
}
